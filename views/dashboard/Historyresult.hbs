<body>

	<div class="header">
    
  
   <a href="dashboard.hbs" style="text-decoration: none;color: white;"><h4>Quister</h4></a>
    <div class="nav-item">
          <a class="nav-link" data-toggle="modal" data-target="#exampleModal" style="margin-right: 10px;">
            <i class="fa fa-fw fa-sign-out"></i>Logout</a>
        </div>
  </div>

  <div class="container">
  <h2 style="display: flex;flex-direction: row;justify-content: space-around;"><strong>Result</strong></h2>
  <form action="#" method="post">

    <input type="text" value="{{quizid}}" name="quiz_id" hidden>
    <div class="form-group">
      <label for="email">Total Question</label>
      <input type="email" class="form-control" id="email" value="{{noofquestion}}" name="no_of_question" readonly>
    </div>
    <div class="form-group">
      <label for="pwd">No of attempted Question</label>
      <input type='text' class="form-control" id="pwd" value="{{noOfAttempted}}" name="pwd" readonly>
    </div>
    <div class="form-group">
      <label for="pwd">Total correct</label>
      <input type="text" class="form-control" id="pwd" value="{{correctanswer}}" name="correct" readonly>
    </div>
    <div class="form-group">
      <label for="pwd">Mark Obtained</label>
      <input type="text" class="form-control" id="pwd" value="{{correctanswer}}/{{noofquestion}}" name="pwd" readonly>
    </div>
    
      
  </form>
   <button class="btn btn-success" onclick="goToDashboard()">Ok</button>
 
</div>
<script>
  

  function goToDashboard(){
       window.location.href="./dashboard.hbs";
  }
</script>
        
       
